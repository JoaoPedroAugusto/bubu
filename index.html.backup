    <script>
        $(document).ready(function () {
            function randomInt(min, max) {
                return Math.floor(Math.random() * (max - min + 1)) + min;
            }

            $('.bg_heart').each(function () {
                var heartCount = ($(this).width() / 50) * 5;
                var colors = ['#ec4899', '#f472b6', '#db2777', '#be185d'];

                for (var i = 0; i <= heartCount; i++) {
                    var size = randomInt(10, 30);
                    var color = colors[randomInt(0, colors.length - 1)];
                    var leftPos = randomInt(0, 97);
                    var topPos = randomInt(-100, -10);
                    var duration = randomInt(5, 15);

                    $(this).append('<div class="heart" style="width:' + size + 'px;height:' + size + 'px;left:' + leftPos + '%;top:' + topPos + '%;background:' + color + ';animation:love ' + duration + 's ease-in infinite;"></div>');
